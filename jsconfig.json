{
    "compilerOptions": {
        "module": "commonjs",
        "target": "ES6",
        "baseUrl": ".",
        "paths": {
            "~/*": ["./*"],
            "@/*": ["./*"],
            "~~/*": ["./*"],
            "@@/*": ["./*"]
        }
    },
    "exclude": ["node_modules", "node_modules/**/*"],
    "extensions": [".vue"]
}
